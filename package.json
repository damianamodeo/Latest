{
  "name": "proclaimer",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "tsc && VITE_APP_BUILD_TIME=$(date +%s) vite build --base=./",
    "preview": "vite preview",
    "deploy-gh-pages": "tsc && VITE_APP_BUILD_TIME=$(date +%s) vite build && gh-pages -d dist -m 'gh-pages deploy'",
    "gh-pages-help": "gh-pages --help",
    "generate-PROCLAIMER-pwa-assets": "npx pwa-asset-generator src/apps/proclaimer/app/logo.svg src/apps/proclaimer/public/assets --background $(grep -Po '^VITE_APP_SPLASH_SCREEN_COLOR=\\K.*' .env  ) --favicon true --padding '10%'"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.0.0",
    "framer-motion": "^10.2.4",
    "gh-pages": "^5.0.0",
    "pwa-asset-generator": "^6.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.9",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-svgr": "^2.4.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/react": "^18.0.29",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "autoprefixer": "^10.4.13",
    "postcss": "^8.4.21",
    "tailwindcss": "^3.2.7",
    "typescript": "^4.9.3",
    "vite": "^4.1.0",
    "vite-plugin-pwa": "^0.14.1"
  }
}
